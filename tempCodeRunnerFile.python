import matplotlib.pyplot as plt

# Sample data
x_values = [1, 2, 3, 4, 5]
y_values = [2, 4, 1, 3, 5]

# Create a stairs plot
plt.step(x_values, y_values, where='mid', label='Stairs Plot')

# Adding labels and title
plt.xlabel('X-axis')
plt.ylabel('Y-axis')
plt.title('Stairs Plot Example')

# Adding legend
plt.legend()

# Display the plot
plt.show()
